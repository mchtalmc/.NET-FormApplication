@model IEnumerable<Product>




    <table class="table table-bordered table-striped">

        <thead>
            <tr>
                <th>@Html.DisplayNameFor(m => m.ProductId)</th>
                <th>@Html.DisplayNameFor(m => m.Image)</th>
                <th>@Html.DisplayNameFor(m => m.ProductName)</th>
                <th>@Html.DisplayNameFor(m => m.ProductPrice)</th>

            </tr>
        </thead>
        <tbody>
            <h2>Hata aliyorum Product bilgilerini render edemedim .</h2>
        </tbody>
    </table>